0/","height":739},"id":"71c92b31","executionInfo":{"status":"ok","timestamp":1751935029928,"user_tz":240,"elapsed":459,"user":{"displayName":"Akshay Sharma","userId":"07431134383062336940"}},"outputId":"7156ec4d-28c9-4f60-af6f-4aeb9687ab67"},"source":["from sklearn.metrics import accuracy_score, precision_score, recall_score, f1_score, roc_auc_score\n","import matplotlib.pyplot as plt\n","import seaborn as sns\n","import pandas as pd\n","\n","y_pred = model.predict(X_test)\n","\n","accuracy = accuracy_score(y_test, y_pred)\n","precision = precision_score(y_test, y_pred)\n","recall = recall_score(y_test, y_pred)\n","f1 = f1_score(y_test, y_pred)\n","roc_auc = roc_auc_score(y_test, model.predict_proba(X_test)[:, 1])\n","\n","print(f\"Accuracy: {accuracy:.4f}\")\n","print(f\"Precision: {precision:.4f}\")\n","print(f\"Recall: {recall:.4f}\")\n","print(f\"F1-score: {f1:.4f}\")\n","print(f\"ROC AUC: {roc_auc:.4f}\")\n","\n","# Visualize the performance metrics\n","metrics_names = ['Accuracy', 'Precision', 'Recall', 'F1-score', 'ROC AUC']\n","metrics_values = [accuracy, precision, recall, f1, roc_auc]\n","\n","plt.figure(figsize=(8, 6))\n","sns.barplot(x=metrics_names, y=metrics_values, palette='viridis')\n","plt.title('Logistic Regression Model Performance Metrics')\n","plt.ylabel('Score')\n","plt.ylim(0, 1)\n","plt.show()"],"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["Accuracy: 0.8704\n","Precision: 1.0000\n","Recall: 0.6667\n","F1-score: 0.8000\n","ROC AUC: 0.9437\n"]},{"output_type":"stream","name":"stderr","text":["/tmp/ipython-input-16-617152803.py:25: FutureWarning: \n","\n","Passing `palette` without assigning `hue` is deprecated and will be removed in v0.14.0. Assign the `x` variable to `hue` and set `legend=False` for the same effect.\n","\n","  sns.barplot(x=metrics_names, y=metrics_values, palette='viridis')\n"]},{"output_type":"display_data","data":{"text/plain":["<Figure size 800x600 with 1 Axes>"],"image/png":"\n"},"meta