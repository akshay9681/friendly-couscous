{"colab":{"base_uri":"https://localhost:8080/","height":211},"id":"1da320d3","executionInfo":{"status":"error","timestamp":1751998228560,"user_tz":240,"elapsed":73,"user":{"displayName":"Akshay Sharma","userId":"07431134383062336940"}},"outputId":"ac863787-26cb-4cbf-b62d-e4418449a248"},"source":["import pandas as pd\n","import matplotlib.pyplot as plt\n","import seaborn as sns\n","from sklearn.metrics import accuracy_score, precision_score, recall_score, f1_score, roc_auc_score\n","\n","# --- Recalculate metrics for Original Models (assuming models are still in memory) ---\n","# If models are not in memory, you would need to retrain them first.\n","# Based on the notebook state, model, rf_model, and xgb_model are likely still defined.\n","\n","# Logistic Regression (Original)\n","y_pred_lr_orig = model.predict(X_test)\n","accuracy = accuracy_score(y_test, y_pred_lr_orig)\n","precision = precision_score(y_test, y_pred_lr_orig)\n","recall = recall_score(y_test, y_pred_lr_orig)\n","f1 = f1_score(y_test, y_pred_lr_orig)\n","roc_auc = roc_auc_score(y_test, model.predict_proba(X_test)[:, 1])\n","\n","# Random Forest (Original)\n","y_pred_rf_orig = rf_model.predict(X_test)\n","accuracy_rf = accuracy_score(y_test, y_pred_rf_orig)\n","precision_rf = precision_score(y_test, y_pred_rf_orig)\n","recall_rf = recall_score(y_test, y_pred_rf_orig)\n","f1_rf = f1_score(y_test, y_pred_rf_orig)\n","roc_auc_rf = roc_auc_score(y_test, rf_model.predict_proba(X_test)[:, 1])\n","\n","# XGBoost (Original)\n","y_pred_xgb_orig = xgb_model.predict(X_test)\n","accuracy_xgb = accuracy_score(y_test, y_pred_xgb_orig)\n","precision_xgb = precision_score(y_test, y_pred_xgb_orig)\n","recall_xgb = recall_score(y_test, y_pred_xgb_orig)\n","f1_xgb = f1_score(y_test, y_pred_xgb_orig)\n","roc_auc_xgb = roc_auc_score(y_test, xgb_model.predict_proba(X_test)[:, 1])\n","\n","\n","# --- Recalculate metrics for Tuned Models (assuming tuned models are still in memory) ---\n","# Based on the notebook state, tuned_lr_model, tuned_rf_model, and tuned_xgb_model are likely still defined.\n","\n","# Logistic Regression (Tuned)\n","y_pred_tuned_lr = tuned_lr_model.predict(X_test)\n","accuracy_tuned_lr = accuracy_score(y_test, y_pred_tuned_lr)\n","precision_tuned_lr = precision_score(y_test, y_pred_tuned_lr)\n","recall_tuned_lr = recall_score(y_test, y_pred_tuned_lr)\n","f1_tuned_lr = f1_score(y_test, y_pred_tuned_lr)\n","roc_auc_tuned_lr = roc_auc_score(y_test, tuned_lr_model.predict_proba(X_test)[:, 1])\n","\n","# Random Forest (Tuned)\n","y_pred_tuned_rf = tuned_rf_model.predict(X_test)\n","accuracy_tuned_rf = accuracy_score(y_test, y_pred_tuned_rf)\n","precision_tuned_rf = precision_score(y_test, y_pred_tuned_rf)\n","recall_tuned_rf = recall_score(y_test, y_pred_tuned_rf)\n","f1_tuned_rf = f1_score(y_test, y_pred_tuned_rf)\n","roc_auc_tuned_rf = roc_auc_score(y_test, tuned_rf_model.predict_proba(X_test)[:, 1])\n","\n","# XGBoost (Tuned)\n","y_pred_tuned_xgb = tuned_xgb_model.predict(X_test)\n","accuracy_tuned_xgb = accuracy_score(y_test, y_pred_tuned_xgb)\n","precision_tuned_xgb = precision_score(y_test, y_pred_tuned_xgb)\n","recall_tuned_xgb = recall_score(y_test, y_pred_tuned_xgb)\n","f1_tuned_xgb = f1_score(y_test, y_pred_tuned_xgb)\n","roc_auc_tuned_xgb = roc_auc_score(y_test, tuned_xgb_model.predict_proba(X_test)[:, 1])\n","\n","\n","# Collect the metrics for each model\n","metrics_data = {\n","    'Metric': ['Accuracy', 'Precision', 'Recall', 'F1-score', 'ROC AUC'] * 6,\n","    'Score': [accuracy, precision, recall, f1, roc_auc,\n","              accuracy_tuned_lr, precision_tuned_lr, recall_tuned_lr, f1_tuned_lr, roc_auc_tuned_lr,\n","              accuracy_rf, precision_rf, recall_rf, f1_rf, roc_auc_rf,\n","              accuracy_tuned_rf, precision_tuned_rf, recall_tuned_rf, f1_tuned_rf, roc_auc_tuned_rf,\n","              accuracy_xgb, precision_xgb, recall_xgb, f1_xgb, roc_auc_xgb,\n","              accuracy_tuned_xgb, precision_tuned_xgb, recall_tuned_xgb, f1_tuned_xgb, roc_auc_tuned_xgb],\n","    'Model': ['Logistic Regression (Original)'] * 5 + ['Logistic Regression (Tuned)'] * 5 +\n","             ['Random Forest (Original)'] * 5 + ['Random Forest (Tuned)'] * 5 +\n","             ['XGBoost (Original)'] * 5 + ['XGBoost (Tuned)'] * 5\n","}\n","\n","metrics_df = pd.DataFrame(metrics_data)\n","\n","# Create the bar plot\n","plt.figure(figsize=(15, 8))\n","sns.barplot(x='Metric', y='Score', hue='Model', data=metrics_df, palette='viridis')\n","plt.title('Model Performance Comparison (Original vs. Tuned)')\n","plt.ylabel('Score')\n","plt.ylim(0, 1)\n","plt.show()"],"execution_count":null,"outputs":[{"output_type":"error","ename":"NameError","evalue":"name 'model' is not defined","traceback":["\u001b[0;31m---------------------------------------------------------------------------\u001b[0m","\u001b[0;31mNameError\u001b[0m                                 Traceback (most recent call last)","\u001b[0;32m/tmp/ipython-input-10-3542937056.py\u001b[0m in \u001b[0;36m<cell line: 0>\u001b[0;34m()\u001b[0m\n\u001b[1;32m      9\u001b[0m \u001b[0;34m\u001b[0m\u001b[0m\n\u001b[1;32m     10\u001b[0m \u001b[0;31m# Logistic Regression (Original)\u001b[0m\u001b[0;34m\u001b[0m\u001b[0;34m\u001b[0m\u001b[0m\n\u001b[0;32m---> 11\u001b[0;31m \u001b[0my_pred_lr_orig\u001b[0m \u001b[0;34m=\u001b[0m \u001b[0mmodel\u001b[0m\u001b[0;34m.\u001b[0m\u001b[0mpredict\u001b[0m\u001b[0;34m(\u001b[0m\u001b[0mX_test\u001b[0m\u001b[0;34m)\u001b[0m\u001b[0;34m\u001b[0m\u001b[0;34m\u001b[0m\u001b[0m\n\u001b[0m\u001b[1;32m     12\u001b[0m \u001b[0maccuracy\u001b[0m \u001b[0;34m=\u001b[0m \u001b[0maccuracy_score\u001b[0m\u001b[0;34m(\u001b[0m\u001b[0my_test\u001b[0m\u001b[0;34m,\u001b[0m \u001b[0my_pred_lr_orig\u001b[0m\u001b[0;34m)\u001b[0m\u001b[0;34m\u001b[0m\u001b[0;34m\u001b[0m\u001b[0m\n\u001b[1;32m     13\u001b[0m \u001b[0mprecision\u001b[0m \u001b[0;34m=\u001b[0m \u001b[0mprecision_score\u001b[0m\u001b[0;34m(\u001b[0m\u001b[0my_test\u001b[0m\u001b[0;34m,\u001b[0m